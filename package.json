    "material-design-icons-iconfont": "^4.0.2",
    "qs": "^6.5.2",
    "register-service-worker": "^1.5.2",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "vue": "^2.5.17",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.1",
    "vue-rx": "^6.0.1",
    "vuetify": "^1.3.5",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^3.0.1",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-knobs": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addons": "^4.0.2",
    "@storybook/vue": "^4.0.2",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.12.2",
    "@types/qs": "^6.5.1",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-e2e-cypress": "^3.1.1",
    "@vue/cli-plugin-pwa": "^3.1.1",
