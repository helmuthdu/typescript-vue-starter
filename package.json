    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "@vue/cli-service": "^3.1.2",
    "@vue/test-utils": "^1.0.0-beta.25",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "7.0.0-bridge.0",
    "cypress-testing-library": "^2.3.1",
    "lint-staged": "^8.0.4",
    "prettier": "^1.14.3",
    "storybook-readme": "^4.0.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^23.10.4",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6",
    "vue-cli-plugin-storybook": "^0.4.8",
    "vue-template-compiler": "^2.5.17",
    "wallaby-vue-compiler": "^1.0.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
